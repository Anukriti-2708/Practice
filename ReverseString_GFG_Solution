class Solution {
    
    public String reverse(String S){
       Stack <Character> st = new Stack<>();
    for(int i=0;i<S.length();i++){
        char ch = S.charAt(i);
        st.push(ch);
         }
         String str = "";
         while (!st.empty()){
        char ch = st.pop();
           str+=ch;
        
         }
         return str;
         
    }

}
